<!--[if IE 7]>    <html class="gallery no-js ie7 oldie" lang="en"> <![endif]-->
<!--[if IE 8]>    <html class="gallery no-js ie8 oldie" lang="en"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="gallery no-js" lang="en"> <!--<![endif]-->
<head>
	<!--
		touch_gallery
		gallery.page
	-->
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

	<title><%= title %></title>
	<meta name="keywords" content="<%=  keywords  %>">
	<meta name="description" content="<%=  description  %>">

	<meta name="viewport" content="width=device-width,initial-scale=1">

	<link rel="stylesheet" href="/templates/touch_gallery/css/style.css">

</head>
<body>

	<div id="wrapper">
		<table>
			<tbody>
				<td><img src="/templates/touch_gallery/img/dummy/p01.jpg"></td>
				<td><img src="/templates/touch_gallery/img/dummy/p02.jpg"></td>
				<td><img src="/templates/touch_gallery/img/dummy/p03.jpg"></td>
			</tbody>
		</table>
	</div> <!--! end of #container -->

	<script src="/templates/touch_gallery/js/libs/iscroll.js" type="text/javascript" charset="utf-8"></script>
	<script>
	var myScroll;
	function loaded() {
		setTimeout(function () {
			myScroll = new iScroll('wrapper',{
				snap     : true,
				vScroll  : false,
				momentum : false
			});
		}, 100);
	}
	window.addEventListener('load', loaded, false);
	</script>
</body>
</html>
